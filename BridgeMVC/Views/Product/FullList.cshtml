@model IEnumerable<BridgeMVC.Models.Product>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BridgeModule)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProdDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NpsJobId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DbJobId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uk)
        </th>
        <th></th>
