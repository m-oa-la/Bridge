        <td>
            @Html.DisplayFor(modelItem => item.Tag)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BridgeModule)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductItem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NpsJobId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DbJobId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Uk)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
